# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Performance Optimization Feature Flags
# FASE 1: Quick Wins (Enabled by default - low risk, high impact)
VITE_ENABLE_PREFETCHING=true
VITE_ENABLE_THROTTLING=true

# FASE 2: Binary Protocol & WebSocket (Disabled by default - requires backend support)
VITE_ENABLE_BINARY_PROTOCOL=false
VITE_ENABLE_WEBSOCKET=false

# FASE 3: Advanced Caching (Disabled by default - requires testing)
VITE_ENABLE_INDEXEDDB_CACHE=false

# Development/Debug
VITE_DEBUG_PERFORMANCE=false
